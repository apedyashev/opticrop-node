{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "object-literal-sort-keys": false,
      "no-var-requires": false,
      "no-consecutive-blank-lines": [true, 2],
      "no-bitwise": false,
      "interface-over-type-literal": false
    },
    "rulesDirectory": []
}
